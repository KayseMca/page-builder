{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/api-interfaces/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api-interfaces"],
          "options": {
            "jestConfig": "libs/api-interfaces/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "material-module": {
      "projectType": "library",
      "root": "libs/material-module",
      "sourceRoot": "libs/material-module/src",
      "prefix": "sognando-casa",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/material-module"],
          "options": {
            "jestConfig": "libs/material-module/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/material-module/src/**/*.ts",
              "libs/material-module/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "page-builder": {
      "projectType": "application",
      "root": "apps/page-builder",
      "sourceRoot": "apps/page-builder/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/page-builder",
            "index": "apps/page-builder/src/index.html",
            "main": "apps/page-builder/src/main.ts",
            "polyfills": "apps/page-builder/src/polyfills.ts",
            "tsConfig": "apps/page-builder/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/page-builder/src/favicon.ico",
              "apps/page-builder/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/page-builder/src/styles.scss"
            ],
            "scripts": ["./node_modules/jquery/dist/jquery.min.js"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/page-builder/src/environments/environment.ts",
                  "with": "apps/page-builder/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "page-builder:build:production"
            },
            "development": {
              "browserTarget": "page-builder:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "page-builder:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/page-builder/src/test.ts",
            "polyfills": "apps/page-builder/src/polyfills.ts",
            "tsConfig": "apps/page-builder/tsconfig.spec.json",
            "karmaConfig": "apps/page-builder/karma.conf.js",
            "assets": [
              "apps/page-builder/src/favicon.ico",
              "apps/page-builder/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/page-builder/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/apps/page-builder-server",
            "main": "server.ts",
            "tsConfig": "apps/page-builder/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/page-builder/src/environments/environment.ts",
                  "with": "apps/page-builder/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "page-builder:build:development",
              "serverTarget": "page-builder:server:development"
            },
            "production": {
              "browserTarget": "page-builder:build:production",
              "serverTarget": "page-builder:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": ["/"]
          },
          "configurations": {
            "production": {
              "browserTarget": "page-builder:build:production",
              "serverTarget": "page-builder:server:production"
            },
            "development": {
              "browserTarget": "page-builder:build:development",
              "serverTarget": "page-builder:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "product": {
      "projectType": "application",
      "root": "apps/product",
      "sourceRoot": "apps/product/src",
      "prefix": "product",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/product",
            "index": "apps/product/src/index.html",
            "main": "apps/product/src/main.ts",
            "polyfills": "apps/product/src/polyfills.ts",
            "tsConfig": "apps/product/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/product/src/favicon.ico",
              "apps/product/src/assets"
            ],
            "styles": ["apps/product/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/product/src/environments/environment.ts",
                  "with": "apps/product/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "apps/production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "product:build:production"
            },
            "development": {
              "browserTarget": "product:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "product:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/product/src/**/*.ts",
              "apps/product/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/product"],
          "options": {
            "jestConfig": "apps/product/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "product-e2e": {
      "root": "apps/product-e2e",
      "sourceRoot": "apps/product-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/product-e2e/cypress.json",
            "devServerTarget": "product:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "product:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/product-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["product"]
    },
    "shared-assets": {
      "root": "libs/shared/assets",
      "sourceRoot": "libs/shared/assets/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/shared/assets/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/assets"],
          "options": {
            "jestConfig": "libs/shared/assets/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
    
  }
}
